{
  "expo": {
    "name": "Quiz-App",
    "slug": "Quiz-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/character_lappy_support.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFAFA"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFAFA"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "React Native Quiz-App",
    "githubUrl": "https://github.com/mdnmnahmed/rn-quiz-app"
  }
}
